<resources>
    <!-- default bluetooth on/off status -->
    <bool name="def_bluetooth_on">false</bool>

    <!-- default WIFI_P2P_DEVICE_NAME -->
    <string name="def_wifi_p2p_device_name">JingPad C1</string>

    <!-- Default for Settings.Secure.SHOW_IME_WITH_HARD_KEYBOARD -->
    <bool name="def_show_ime_with_hard_keyboard">true</bool>

    <!-- Default for Settings.Secure.DOZE_ENABLED -->
    <bool name="def_doze_enabled">true</bool>

    <!-- Default for Settings.Secure.TP_WAKE_GESTURE_ENABLED -->
    <bool name="def_tp_wake_gesture_enabled">false</bool>


     <!-- Default screen brightness, from 0 to 255.  71 is 75%. -->
    <integer name="def_screen_brightness">71</integer>

    <!-- Default value for whether or not to pulse the notification LED when there is a
         pending notification -->
    <bool name="def_notification_pulse">false</bool>

</resources>
